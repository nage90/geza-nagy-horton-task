    "@angular/flex-layout": "5.0.0-beta.14",
    "@angular/forms": "5.2.0",
    "@angular/http": "5.2.0",
    "@angular/material": "5.2.0",
    "@angular/platform-browser": "5.2.0",
    "@angular/platform-browser-dynamic": "5.2.0",
    "@angular/router": "5.2.0",
    "@covalent/core": "1.0.0",
    "detect-libc": "^1.0.3",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
