  },
  "license": "Apache License, Version 2.0",
  "author": "Scott Aslan",
  "contributors": [
    "Scott Aslan <scottyaslan@gmail.com>"
  ],
  "dependencies": {
    "@angular/animations": "5.2.0",
    "@angular/cdk": "5.2.0",
    "@angular/common": "5.2.0",
    "@angular/compiler": "5.2.0",
